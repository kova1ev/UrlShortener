{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "ApplicationName": "URL SHORTENER"
    }
  },
  "AllowedHosts": "*",
  "AppOptions": {
    "AppUrl": "hostname"
  },
  "ApiKeyOptions": {
    "Apikey": "secret"
  },
  "JwtOptions": {
    "Issuer": "hostname",
    "Audience": "hostname",
    "SecretKey": "mysupersecretkey",
    "LifeTime": "60"
  },
  "Admin": {
    "Email": "secret",
    "Password": "secret",
    "Role": "admin"
  },
  "ConnectionStrings": {
    "PostgresSql": "secret"
  },
  "InMemory": false
}